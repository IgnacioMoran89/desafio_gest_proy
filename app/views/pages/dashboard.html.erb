<h1>Mis Proyectos</h1>

<table class ='table'>
    <thead>
        <tr>
            <th>Nombre</th>
            <th>Descripción</th>
            <th>Fecha de Partida</th>
            <th>Fecha de Término</th>
            <th>Estado</th>
        </tr>
    </thead>
    <tbody>
        
        <% @projects.each do |p| %>
        <tr>
            <td><%= p.name %></td>
            <td><%= p.description %></td>
            <td><%= p.started_at %></td>
            <td><%= p.finished_at %></td>
            <td><%= p.state %></td>
        </tr>
        <% end %>
        
    </tbody>
</table>


